    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "prop-types": ">=15.8.1",
    "react": "^18.3.1",
    "react-big-calendar": "^1.12.2",
    "react-chartjs-2": "^5.2.0",
    "react-cropper": "^2.3.3",
    "react-csv": "^2.2.2",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.3.1",
    "react-dom-confetti": "^0.2.0",
    "react-dropzone": "^14.2.3",
